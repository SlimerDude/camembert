## This is a setting files. The format is key = value
## where the value is formatted in JSON format.

# Default font : for anyhting but the editor
font = "9pt {{font}}"

# Default font color : for anyhting but the editor
fontColor = "#e8f8f2"

# Background color : for anyhting but the editor
bg = "#272822"

# Background of space 'pills'
spacePillBg = "#000000"

# Background of selected items
selectedItem = "#4e5d62"

# Font used in editor pane'
edFont = "bold 10pt {{font}}"

# Editor pane background color'
edBg = "#272822"

# Editor pane background behind current line'
edCurLineBg = "#3e3d32"

# Background Color for text selection in Editor'
edSelectBg = "#323d3e"

# Where to show color indicators (lines in the editor background)
edCols = [2,79]

# Color for the color indicators
edColsColor = "#666666"

# Default color of text in the editor
edText = {"fg":"#e8f8f2",
"bg":null,
"font":null,
"underlineColor":null,
"underline":"none"}

# brackets in editor
edBracket = {"fg":"#33AA33",
"bg":null,
"font":null,
"underlineColor":null,
"underline":"none"}

# brackets match in editor
edBracketMatch = {"fg":"#33AA33",
"bg":"#774444",
"font":null,
"underlineColor":null,
"underline":"none"}

# keywords in editors
edKeyword = {"fg":"#f92672",
"bg":null,
"font":null,
"underlineColor":null,
"underline":"none"}

# Numbers in editor
edNum = {"fg":"#9581f7",
"bg":null,
"font":null,
"underlineColor":null,
"underline":"none"}

# Strings in editor
edStr = {"fg":"#e6db74",
"bg":null,
"font":null,
"underlineColor":null,
"underline":"none"}

# Comments in editor
edComment = {"fg":"#75715e",
"bg":null,
"font":null,
"underlineColor":null,
"underline":"none"}

# Color of line number addicator
lineNumberColor = "#666666"


# Color of the caret
caretColor = "#ffffff"

# Color of the scrollbar bg
scrollBg = "#444444"

# Color of the scrollbar handle
scrollFg = "#666666"

# Color for help docs bg 1
helpBg1 = "#444444"

# Color for help docs bg 1
helpBg2 = "#777777"